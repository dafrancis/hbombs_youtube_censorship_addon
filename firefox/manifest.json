{
  "manifest_version": 2,
  "name": "Hbomb's YouTube Censorship Addon",
  "description": "This extension hides videos from the Skulls Parasite Unit.",
  "version": "1.3",
  "icons": { "128": "hbomb.png" },
  "applications": {
    "gecko": {
      "id": "hbca@somethingafal.com"
    }
  },
  "content_scripts":
  [
    {
      "matches": ["*://*.youtube.com/*", "*://youtube.com/*"],
      "js": ["script.js"],
      "run_at": "document_end"
    }
  ]
}
